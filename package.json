{
    "name": "@sellerspot/universal-components",
    "version": "1.16.16",
    "description": "Universal Components for sellerspot eco-system.",
    "main": "dist/index.js",
    "scripts": {
        "dev": "webpack --mode development --env development",
        "build:dev": "webpack --mode development  --env production && yalc push",
        "build": "webpack --mode production --env NODE_ENV=production --env production",
        "build:analyze": "webpack --mode production --env NODE_ENV=production --env production --env analyze",
        "lint": "eslint src --max-warnings 0",
        "lint:fix": "eslint src --fix --max-warnings 0",
        "storybook": "start-storybook -p 9007",
        "build:storybook": "build-storybook",
        "prettier": "prettier --write src",
        "lint-staged": "lint-staged",
        "prepare": "npm run husky:install",
        "husky:install": "husky install",
        "stylelint:fix": "npx stylelint \"**/*.scss\" --fix",
        "husky:pre-commit": "npm run lint-staged",
        "update:packages": "npm i -D @sellerspot/webpack-run-scripts-custom-plugin"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SellerSpot/universal-components.git"
    },
    "keywords": [
        "sellerspot",
        "react-universal-components"
    ],
    "author": "Sellerspot",
