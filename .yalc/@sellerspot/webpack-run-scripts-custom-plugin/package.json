{
    "name": "@sellerspot/webpack-run-scripts-custom-plugin",
    "version": "1.18.3",
    "description": "Custom plugin for running scripts on successfull compilation and file emition",
    "main": "dist/index.js",
    "scripts": {
        "dev": "webpack --mode development --env development && yalc push",
        "build": "webpack --mode production --env NODE_ENV=production --env production",
        "build:analyze": "webpack --mode production --env NODE_ENV=production --env production --env analyze",
        "lint": "eslint src --max-warnings 0",
        "lint:fix": "eslint src --fix --max-warnings 0",
        "prettier": "prettier --write src",
        "lint-staged": "lint-staged",
        "husky:install": "husky install",
        "husky:pre-commit": "npm run lint-staged"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SellerSpot/webpack-run-scripts-custom-plugin.git"
    },
    "keywords": [
        "sellerspot",
        "react-webpack-run-scripts-custom-plugin"
    ],
    "author": "Sellerspot",
    "license": "ISC",
    "peerDependencies": {
        "@types/webpack": "^4.41.25",
        "webpack": "^4.46.0"
    },
    "bugs": {
        "url": "https://github.com/SellerSpot/webpack-run-scripts-custom-plugin/issues"
    },
    "homepage": "https://github.com/SellerSpot/webpack-run-scripts-custom-plugin#readme",
    "dependencies": {},
    "yalcSig": "4c66e71e9dcabb08b18a571c8ed0a311"
}
